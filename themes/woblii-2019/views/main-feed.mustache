<section id="feedContainer" class="mb-24 mx-auto relative md:pt-14 pt-32 pl-6 md:pl-0 pr-6 md:pr-0 w-full">
    <mini-profile
        :user="{{ userObject }}"
        >
    </mini-profile>
    <div id="mainFeed" class="block w-full max-w-3xl mx-auto">
        <nav id="feedNavigation">
            <ul class="list-none p-0 m-0 mt-4">
                <li class="inline-block align-baseline w-16 h-16 mr-4 mb-2 rounded-lg border border-purple-dark bg-purple-dark">
                    <a title="Tu Universo" class="inline-block m-2" href="{{ site_url }}/my-feed"><i class="customIcons feed white"></i></a>
                </li>
                <li class="inline-block align-baseline w-16 h-16 mr-4 mb-2 rounded-lg border border-purple-dark">
                    <a title="Explora Proyectos" class="inline-block m-2" href="{{ site_url }}/manage-projects"><i class="customIcons projects"></i></a>
                </li>
                <li class="inline-block align-baseline w-16 h-16 mr-4 mb-2 rounded-lg border border-purple-dark">
                    <a title="Mi Actividad" class="inline-block m-2" href="{{ site_url }}/interactions"><i class="customIcons interactions"></i></a>
                </li>
                <li class="inline-block align-baseline w-16 h-16 mr-4 mb-2 rounded-lg border border-purple-dark">
                    <a title="Mis Wobliis" class="inline-block m-2" href="#"><i class="customIcons woblii"></i></a>
                </li>
            </ul>
        </nav>

        <section id="postContainer" class="border border-purple-dark rounded-lg">

            <h2 class="font-hairline text-black text-xl md:text-3xl p-4">Tu universo:</h2>

            {{#posts}}

                {{# feed_post }}
                    {{>each-feed-post }}
                {{/ feed_post }}
                {{^ feed_post }}
                    {{>each-project-post }}
                {{/ feed_post }}

            {{/posts}}

        </section>

    </div>

</section>
