<section id="feedContainer" class="mb-24 mx-auto relative md:pt-14 pt-32 pl-6 md:pl-0 pr-6 md:pr-0 w-full">
    <mini-profile
        :user="{{ userObject }}"
        >
    </mini-profile>
    <div id="mainFeed" class="block w-full max-w-3xl mx-auto">
        <nav id="feedNavigation">
			<nav id="feedNavigation">
	            <ul class="list-none p-0 m-0 mt-4">
	                <li class="inline-block align-baseline w-12 h-12 mr-4 mb-2 rounded-lg border border-purple-dark">
	                    <a title="Tu Universo" href="{{ site_url }}/my-feed"><i class="customIcons feed white"></i></a>
	                </li>
	                <li class="inline-block align-baseline w-12 h-12 mr-4 mb-2 rounded-lg border border-purple-dark bg-purple-dark">
	                    <a title="Explora Proyectos" href="{{ site_url }}/manage-projects"><i class="customIcons projects"></i></a>
	                </li>
	                <li class="inline-block align-baseline w-12 h-12 mr-4 mb-2 rounded-lg border border-purple-dark">
	                    <a title="Mi Actividad" href="{{ site_url }}/interactions"><i class="customIcons interactions"></i></a>
	                </li>
	                <li class="inline-block align-baseline w-12 h-12 mr-4 mb-2 rounded-lg border border-purple-dark">
	                    <a title="Mis Wobliis" href="#"><i class="customIcons woblii"></i></a>
	                </li>
	            </ul>
	        </nav>
        </nav>

        <section id="postContainer" class="border border-purple-dark rounded-lg">

            <h2 class="font-hairline text-black text-xl md:text-3xl p-4">Tus proyectos: <a title="Compartir un proyecto" href="{{ site_url }}/create-project"><i class="material-icons text-purple-dark text-xl">add_circle</i></a></h2>

            {{#posts}}

                {{>each-project-post }}

            {{/posts}}

            {{^ posts }}
